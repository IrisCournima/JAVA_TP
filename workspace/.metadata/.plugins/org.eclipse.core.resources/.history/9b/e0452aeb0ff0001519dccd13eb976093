package Modele;

import java.util.Date;

public class Conge {
	private int id;
	private int empId;
	private Date debut;
	private Date fin;
	private String etat;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getEmpId() {
		return empId;
	}
	public void setEmpId(int empId) {
		this.empId = empId;
	}
	public Date getDebut() {
		return debut;
	}
	public void setDebut(Date debut) {
		this.debut = debut;
	}
	public Date getFin() {
		return fin;
	}
	public void setFin(Date fin) {
		this.fin = fin;
	}
	public String getEtat() {
		return etat;
	}
	public void setEtat(String etat) {
		this.etat = etat;
	}
	public Conge(int id, int empId, Date debut, Date fin, String etat) {
		super();
		this.id = id;
		this.empId = empId;
		this.debut = debut;
		this.fin = fin;
		this.etat = etat;
	}
	public Conge() {
		super();
	}



	
	
}
